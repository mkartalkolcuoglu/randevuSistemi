{
  "name": "project-admin",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3002",
    "build": "prisma generate && next build",
    "vercel-build": "prisma db push --skip-generate && prisma generate && npm run db:seed || true && next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-admin": "tsx scripts/seed-admin.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@repo/ui": "*",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^12.4.1",
    "next": "^15.5.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.31",
    "prisma": "^6.16.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "eslint": "^9.34.0"
  }
